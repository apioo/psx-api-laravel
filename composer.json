{
  "name": "psx/api-laravel",
  "description": "Laravel package to build type-safe APIs",
  "keywords": [
    "laravel",
    "api",
    "rest",
    "typeapi",
    "code-generator",
    "specification",
    "openapi",
    "dto"
  ],
  "homepage": "https://phpsx.org",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "https://chrisk.app"
    }
  ],
  "require": {
    "php": ">=8.2",
    "psx/api": "^7.2",
    "psx/data": "^7.0",
    "psx/schema": "^7.2",
    "illuminate/console": "^12.0",
    "illuminate/http": "^12.0",
    "illuminate/routing": "^12.0",
    "illuminate/support": "^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "vimeo/psalm": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "PSX\\ApiLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PSX\\ApiLaravel\\Tests\\": "tests/"
    }
  }
}
